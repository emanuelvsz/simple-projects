programa {
  funcao inicio() {
    real num1, num2, num3, res
    
    escreva("Digite o primeiro valor\n")
    leia(num1)
    escreva("Digite o segundo valor\n")
    leia(num2)
    escreva("Digite o terceiro valor\n")
    leia(num3)

    res = (num1*num1)+(num2*num2)+(num3*num3)
    escreva(res)
  }
}
